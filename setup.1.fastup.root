#!/bin/bash
[ $(id -u) == 0 ] || { echo Please run as root.; exit 1; }
USERNAME=$(cat /etc/username)
set -x

# Extract files
cd /root/fastup
gpg --import --pinentry-mode loopback dat/github.gpg
git-crypt unlock

set +x
cat << EOF

Done. Prepare OAuth2:

	su - rap      
	cd git/fastup/lib/
	./setup.2.ks
	./setup.3.git

EOF
